<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
  <title>Index page</title>
</head>
<body>
  <div class="container">
    <a href="/student/create">
      <button type="button" class="btn btn-success">(+) Add Student</button>
    </a> 
    <table class="table table-striped table-dark">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Name</th>
          <th scope="col">Created At</th>
          <th scope="col">Updated At</th>
          <th scope="col">Action</th>
        </tr>
      </thead>
      <tbody>
        <% students.forEach(function(student) { %>
          <tr>
            <th scope="row"><%= student.dataValues.id %></th>
            <td><%= student.dataValues.name %></td>
            <td><%= student.dataValues.createdAt %></td>
            <td><%= student.dataValues.updatedAt %></td>
            <td>
              <a href="">
                <button type="button" class="btn btn-primary btn-sm">Update</button>
              </a>
            </td>
            <td>
              <a href="student/delete/<%= student.dataValues.id %>">
                <button type="button" class="btn btn-secondary btn-sm">Delete</button>
              </a>
            </td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </div>   
</body>
</html>